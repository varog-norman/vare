	"tokenColors": [
		{
			"name": "Comments",
			"scope": "comment",
			"settings": {
				"foreground": "#b2b2b2"
			}
		},
		{
			"name": "strings",
			"scope": "string",
			"settings": {
				"foreground": "#8dc891"
			}
		},
		{
			"name": "Generic: Heading (multi line)",
			"scope": "markup.heading.1 punctuation.definition.heading",
			"settings": {
				"foreground": "#ff0000"
			}
		},
		{
			"name": "Name.Entity",
			"scope": [
        "constant.character.entity",
        "entity.name.exception"
      ],
			"settings": {
				"foreground": "#d67c9b"
			}
		},
		{
			"name": "Storage.Modifier",
			"scope": [
				"storage.modifier",
				"storage.type",
				"keyword",
				"storage.type.class"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#c5a5c5"
			}
		},
		{
			"name": "Storage.Type",
			"scope": [
				"constructor",
				"entity.name.function",
				"support.function",
				"support.class.builtin",
				"keyword.other.name-of-parameter",
        "keyword.operator.new",
        "function"
			],
			"settings": {
				"foreground": "#79b6f2"
			}
		},
		{
			"name": "Variable.Language",
			"scope": [
				"variable.language",
				"entity.other.attribute-name",
				"constant.language.undefined",
				"constant.language.null"
			],
			"settings": {
				"foreground": "#c5a5c5",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"punctuation.definition.comment",
				"comment.line",
				"comment.line.double-slash"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#b2b2b2"
			}
		},
		{
			"name": "Decorator",
			"scope": [
				"punctuation",
				"punctuation.definition.tag",
				"punctuation.section.embedded",
				"meta.brace.round",
				"meta.array.literal",
				"entity.name.class.decorator",
				"variable.other.readwrite.decorator"
			],
			"settings": {
				"foreground": "#88c6be"
			}
		},
		{
			"name": "Constant.Object.Keys",
			"scope": [
				"string.unquoted",
				"constant.other.object.key"
			],
			"settings": {
				"foreground": "#8ADF69"
			}
		},
		{
			"name": "Variable.Other.Class",
			"scope": [
				"variable.other.class",
				"meta.property.class",
        "entity.name.tag"
			],
			"settings": {
				"foreground": "#A4FFFF"
			}
		},
		{
			"name": "Console",
			"scope": [
				"support.type.object.console"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Function.Arrow",
			"scope": [
				"storage.type.function.arrow",
				"meta.function.arrow"
			],
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"entity.name.type",
				"support.type",
				"support.class.builtin",
				"entity.other.inherited-class",
				"support.type.property-name.json.comments",
				"support.class",
				"variable.language.this",
				"entity.name.class",
				"support.class",
				"support.class.component",
				"constant.numeric",
				"punctuation.terminator.statement",
				"support.variable.property.dom",
				"entity.other.inherited-class",
				"entity.name.type",
				"type",
			],
			"settings": {
				"foreground": "#F2A4AC"
			}
		},
		{
			"scope": [
        "variable",
        "variables",
				"variable.other",
				"variable.other.constant",
				"variable.other.property",
				"variable.parameter",
				"variable.other.readwrite",
				"meta.object-literal.key",
			],
			"settings": {
				"foreground": "#fffffff0"
			}
		},
		{
			"scope": [
				"keyword.operator",
				"punctuation.terminator.statement",
				"meta.decorator variable.other.readwrite",
				"meta.decorator variable.other.object",
				"meta.decorator variable.other.property"
			],
			"settings": {
        "foreground": "#88c6be",
        "fontStyle": ""
			}
		},
		{
			"scope": [
				"constant.numeric",
				"constant.language.boolean",
				"token.warn-token"
			],
			"settings": {
				"foreground": "#FFCCBC"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#6796e6"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#f44747"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#b267e6"
			}
		}
	]
}